{
  "name": "fnbr/webtool",
  "description": "FrameNet Brasil Web Annotation Tool",
  "license": "GPLv3",
  "config": {
    "platform": {
      "php": "7.3.5"
    }
  },
  "keywords": [
    "framenet",
    "annotation",
    "frames",
    "constructions"
  ],
  "authors": [
    {
      "name": "Tiago Torrent",
      "email": "tiago.torrent@gmail.com"
    },
    {
      "name": "Ely Matos",
      "email": "admin@ematos.net"
    }
  ],
  "require": {
    "php": "7.3.*",
    "psr/log": "1.0.0",
    "phpmailer/phpmailer": "~6.0",
    "zendframework/zend-session": "2.8.*",
    "zendframework/zend-config": "2.6.*",
    "zendframework/zend-validator": "2.8.*",
    "zendframework/zend-captcha": "2.6.*",
    "beberlei/assert": "dev-master",
    "php-di/php-di": "5.2.*",
    "latte/latte": "2.4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "files": [
      "maestro/classes/functions.php"
    ],
    "psr-4": {
      "database\\": "maestro/classes/database",
      "fnbr\\": "",
      "fnbr\\auth\\": "modules/auth/"
    },
    "classmap": [
      "maestro/classes/manager.php",
      "maestro/classes/interfaces",
      "maestro/classes/database",
      "maestro/classes/services",
      "maestro/classes/mvc",
      "maestro/classes/results",
      "maestro/classes/persistence",
      "maestro/classes/extensions",
      "maestro/classes/security",
      "maestro/classes/types",
      "maestro/classes/ui",
      "maestro/classes/utils",
      "components",
      "filters"
    ]
  },
  "scripts": {
    "post-autoload-dump": [
      "Manager::postAutoloadDump"
    ]
  }
}

